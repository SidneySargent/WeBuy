<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.webuy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Promo"></activity>
        <activity
            android:name=".Drawer"
            android:label="@string/title_activity_drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Options_activity" />
        <activity android:name=".BonPlans_activity" />
        <activity android:name=".Profil_activity" />
        <activity android:name=".AchatGroupeDetail" />
        <activity android:name=".AchatGroupe_activity" />
        <activity android:name=".Login_activity" />
        <activity
            android:name=".Magasins_activity"
            android:parentActivityName=".Accueil_activity" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Accueil_activity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>